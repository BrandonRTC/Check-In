<div class="row">
	<div class="small-8 columns small-centered">
		<h2>End Tour</h2>
	</div>
</div>

<div class="row">
	<div class="small-8 columns small-centered">
		<form action="<%= tour_url %>" method="post" enctype="multipart/form-data">
			<%= update_method_input %>
			<%= auth_token %>

			<input type="file" name="tour[end_img]" id="take-picture" accept='image/png, image/jpeg' required>
			<input type="submit" value="Upload" class="button">
		</form>
	</div>
</div>